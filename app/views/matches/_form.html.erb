<%= simple_form_for(@match) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date_inserted %>
    <%= f.input :date_played %>
    <%= f.association :white_player %>
    <%= f.association :black_player %>
    <%= f.input :white_result %>
    <%= f.input :black_result %>
    <%= f.input :round_id %>
    <%= f.input :png_text %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
